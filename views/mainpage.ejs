<link rel="stylesheet" href="/styles.css">

<body>
    
    <div class="container">
        <div class="wrapper">
            <h1>Welcome, <%= user.email%> to the main page!</h1>
            <div class="content">
                <button id="logout-btn" class="dark align-center">Logout</button>
            </div> 
        </div>
    </div>
    <script>
        const logOutBtn = document.getElementById("logout-btn");

        logOutBtn.onclick = function(e){
            location.assign('/logout');
        }

    </script>
</body>